{
	"extends": "./.svelte-kit/tsconfig.json",
	
	"compilerOptions": {
		"paths": {
			"$assets": ["src/assets"],
			"$assets/*": ["src/assets/*"],
			"$content": ["src/content"],
			"$content/*": ["src/content/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$models": ["src/models"],
			"$models/*": ["src/models/*"]
		}
	}
}
